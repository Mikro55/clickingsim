local Section = Tab:NewSection("Worlds")

Section:NewButton("Spawn", "ButtonInfo", function()
    game:GetService("ReplicatedStorage").RemoteFunctions.WorldService.TeleportToWorld:InvokeServer("Spawn");
    print("Teleported")
end)

Section:NewButton("Forest", "ButtonInfo", function()
local TP = CFrame.new(579, 73, 2409)
local info = TweenInfo.new(1, Enum.EasingStyle.Linear)
pcall(function()
        game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character["HumanoidRootPart"], info, {CFrame = TP}):Play()
end)
    print("Clicked")
end)

