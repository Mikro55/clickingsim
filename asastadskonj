local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("TITLE", "DarkTheme")
local Tab = Window:NewTab("Player stuff")
local Section = Tab:NewSection("Section Name")
Section:NewSlider("Speed", "Yourdad", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewSlider("Jump", "mom", 500, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

Section:NewSlider("HipHeight", "SliderInfo", 500, 2, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.HipHeight = s
end)

local Tab = Window:NewTab("Clicking sim")

local Section = Tab:NewSection("AutoClicking")

Section:NewToggle("Auto Clicking", "ToggleInfo", function(state)
    if state then
getgenv().AutoClick = true;

while wait() do
    if getgenv().AutoClick == true then
        game:GetService("ReplicatedStorage").RemoteFunctions.ClickService.Click:InvokeServer();
    end
end
        print("Toggle On")
    else
getgenv().AutoClick = false;
        print("Toggle Off")
    end
end)

local Section = Tab:NewSection("BuyPets")

Section:NewToggle("Basic Egg", "ToggleInfo", function(state)
    if state then
getgenv().BuyBasic = true;

while wait() do
    if getgenv().BuyBasic == true then
        game:GetService("ReplicatedStorage").RemoteFunctions.EggService.HatchEgg:InvokeServer("Basic Egg");
    end
end
        print("Toggle On")
    else
getgenv().BuyBasic = false;
        print("Toggle Off")
    end
end)

